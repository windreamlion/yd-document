{
  "name": "yd-canvas",
  "version": "1.0.0",
  "description": "canvas for Youde",
  "scripts": {
    "clean": "rimraf lib",
    "build:uncompressed": "cross-env NODE_ENV=production webpack --config webpack.config.build.js",
    "build:minified": "cross-env NODE_ENV=production webpack --config webpack.config.build.js --minimize",
    "build": "npm run clean && npm run build:uncompressed && npm run build:minified ",
    "start": "node devServer.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "canvas",
    "youde-component"
  ],
  "author": "lion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/windreamlion/yd-document/issues"
  },
  "homepage": "https://github.com/windreamlion/yd-document#readme",
  "devDependencies": {
    "autoprefixer": "~6.3.1",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "my-local-ip": "^1.0.0",
    "open": "0.0.5",
    "open-browser-webpack-plugin": "0.0.2",
    "html-webpack-plugin": "^2.21.0"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-fabricjs": "^0.1.2"
  }
}
